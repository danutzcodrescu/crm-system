{
  "name": "crm",
  "version": "0.12.9",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "package": "rimraf ./dist && pnpm typecheck && pnpm build",
    "build": "pnpm build-remix && pnpm build-express",
    "build-remix": "remix vite:build",
    "build-express": "tsc -p ./tsconfig-express.json && tsc-alias -p ./tsconfig-express.json",
    "dev": "env-cmd -f .env.local tsx ./cmd/server.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "node --experimental-specifier-resolution=node ./dist/cmd/server.js",
    "start-local": "env-cmd -f .env.local node --experimental-specifier-resolution=node ./dist/cmd/server.js",
    "typecheck": "concurrently pnpm:typecheck-remix pnpm:typecheck:server",
    "typecheck-remix": "tsc",
    "typecheck:server": "tsc -p ./tsconfig-express.json --noEmit",
    "db:push": "env-cmd -f .env.local drizzle-kit push --config=drizzle-dev.config.ts ",
    "db:generate": "env-cmd -f .env.local drizzle-kit generate --config=drizzle-dev.config.ts ",
    "db:migrate": "env-cmd -f .env.local drizzle-kit migrate --config=drizzle-dev.config.ts ",
    "db:seed": "env-cmd -f .env.local tsx ./scripts/seed.ts",
    "db:replace": "env-cmd -f .env.local tsx ./scripts/replaceNames.ts",
    "db:consultation": "env-cmd -f .env.local tsx ./scripts/importInitialConsultationDates.ts",
    "build-image": "env-cmd -f .env.production tsx ./scripts/build.ts"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@date-fns/utc": "^2.1.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@infisical/sdk": "^3.0.91",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^7.29.4",
    "@node-rs/argon2": "^2.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@react-hookz/web": "^25.1.1",
    "@react-pdf/renderer": "^4.3.1",
    "@remix-run/dev": "^2.17.1",
    "@remix-run/express": "^2.17.1",
    "@remix-run/node": "^2.17.1",
    "@remix-run/react": "^2.17.1",
    "@sentry/remix": "^10.19.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@tanstack/react-table": "^8.21.3",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.6",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "helmet": "^8.1.0",
    "html-entities": "^2.6.0",
    "isbot": "^5.1.31",
    "lodash-es": "^4.17.21",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "pino": "^9.13.1",
    "pino-http": "^10.5.0",
    "pino-http-print": "^3.1.0",
    "pino-pretty": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^8.8.0",
    "sanitize-html": "^2.17.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.0",
    "@remix-run/testing": "^2.17.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.18.9",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.5",
    "env-cmd": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "zx": "^8.8.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@sentry/cli"
    ]
  }
}
