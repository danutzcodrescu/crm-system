{
  "name": "crm",
  "version": "0.12.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "package": "rimraf ./dist && pnpm typecheck && pnpm build",
    "build": "pnpm build-remix && pnpm build-express",
    "build-remix": "remix vite:build",
    "build-express": "tsc -p ./tsconfig-express.json && tsc-alias -p ./tsconfig-express.json",
    "dev": "env-cmd -f .env.local tsx ./cmd/server.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "node --experimental-specifier-resolution=node ./dist/cmd/server.js",
    "start-local": "env-cmd -f .env.local node --experimental-specifier-resolution=node ./dist/cmd/server.js",
    "typecheck": "concurrently pnpm:typecheck-remix pnpm:typecheck:server",
    "typecheck-remix": "tsc",
    "typecheck:server": "tsc -p ./tsconfig-express.json --noEmit",
    "db:push": "env-cmd -f .env.local drizzle-kit push --config=drizzle-dev.config.ts ",
    "db:generate": "env-cmd -f .env.local drizzle-kit generate --config=drizzle-dev.config.ts ",
    "db:migrate": "env-cmd -f .env.local drizzle-kit migrate --config=drizzle-dev.config.ts ",
    "db:seed": "env-cmd -f .env.local tsx ./scripts/seed.ts",
    "db:replace": "env-cmd -f .env.local tsx ./scripts/replaceNames.ts",
    "db:consultation": "env-cmd -f .env.local tsx ./scripts/importInitialConsultationDates.ts",
    "build-image": "env-cmd -f .env.production tsx ./scripts/build.ts"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@date-fns/utc": "^2.1.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@infisical/sdk": "^3.0.8",
    "@mui/icons-material": "^7.0.2",
    "@mui/lab": "7.0.0-beta.11",
    "@mui/material": "^7.0.2",
    "@mui/x-date-pickers": "^7.28.0",
    "@node-rs/argon2": "^2.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@react-hookz/web": "^25.1.1",
    "@react-pdf/renderer": "^4.3.0",
    "@remix-run/dev": "^2.16.5",
    "@remix-run/express": "^2.16.5",
    "@remix-run/node": "^2.16.5",
    "@remix-run/react": "^2.16.5",
    "@sentry/remix": "^9.12.0",
    "@sentry/vite-plugin": "^3.3.1",
    "@tanstack/react-table": "^8.21.2",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.0.0",
    "compression": "^1.8.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.41.0",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "helmet": "^8.1.0",
    "html-entities": "^2.6.0",
    "isbot": "^5.1.26",
    "lodash-es": "^4.17.21",
    "node-cron": "^3.0.3",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-http-print": "^3.1.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^8.5.0",
    "sanitize-html": "^2.15.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@remix-run/testing": "^2.16.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.12",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@types/sanitize-html": "^2.15.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.30.6",
    "env-cmd": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "zx": "^8.5.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@sentry/cli"
    ]
  }
}
