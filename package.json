{
  "name": "crm",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "env-cmd -f .env.local tsx ./cmd/server.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "npx tsx ./cmd/server.ts",
    "start-local": "env-cmd -f .env.local remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "db:push": "env-cmd -f .env.local drizzle-kit push --config=drizzle-dev.config.ts ",
    "db:seed": "npx tsx ./scripts/seed.ts"
  },
